package ooppractice;

public class Tao {
	private String name;
	private int age;
	private String gender;
	

	public static void main(String[] args) {
	Tao oat = new Tao();
	oat.setGender("Male");
	
	
	
	

	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
